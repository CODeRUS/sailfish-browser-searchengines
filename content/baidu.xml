<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Baidu</os:ShortName>
<os:Description>Baidu Search</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://www.baidu.com/s</SearchForm>
<os:Url type="application/x-suggestions+json" method="GET" template="http://www.baidu.com/s?word={searchTerms}">
</os:Url><os:Url type="text/html" method="GET" template="http://www.baidu.com/s">
  <os:Param name="word" value="{searchTerms}"/>
</os:Url>
</SearchPlugin>