<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
<ShortName>Qwant.com</ShortName>
<Description>Le moteur de recherche qui facilite la découverte et le partage grâce à une approche sociale</Description>
<InputEncoding>UTF-8</InputEncoding>
<img src="data:image/x-icon;base64," alt=""/>
<Url type="application/x-suggestions+json" method="GET" template="https://api.qwant.com/api/suggest">
  <Param name="q" value="{searchTerms}"/>
  <Param name="client" value="opensearch"/>
</Url>
<Url type="text/html" method="GET" template="https://www.qwant.com/">
  <Param name="q" value="{searchTerms}"/>
  <Param name="client" value="opensearch"/>
</Url>
<moz:SearchForm>https://www.qwant.com/</moz:SearchForm>
</OpenSearchDescription>